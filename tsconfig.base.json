{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "customConditions": [
      "source"
    ],
    "target": "ES2020",
    "module": "Preserve"
  }
}