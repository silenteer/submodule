{
  "name": "@submodule/client",
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "tsd": "^0.28.1"
  },
  "scripts": {
    "build": "yarn run -T tsup",
    "client:build": "yarn build"
  },
  "main": "dist/index.js",
