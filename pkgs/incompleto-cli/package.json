{
  "name": "incompleto-cli",
  "version": "1.0.0",
  "module": "main.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "source": "./src/main.ts",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "incompleto": "dist/main.cjs"
  },
  "dependencies": {
    "@antfu/ni": "^0.23.0",
    "@submodule/core": "workspace:*",
    "@types/degit": "^2.8.6",
    "@types/git-url-parse": "^9.0.3",
    "commander": "^12.1.0",
    "debug": "^4.3.7",
    "degit": "^2.8.4",
    "execa": "^9.4.0",
    "git-url-parse": "^15.0.0",
    "ora": "^8.1.0",
    "zod": "^3.23.8"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "minify": false,
    "treeshake": false,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "scripts": {
    "build": "bunx tsup",
    "dev": "bunx tsup --watch",
    "clean": "bunx rimraf dist"
  }
}