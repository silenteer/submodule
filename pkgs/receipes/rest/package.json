{
  "name": "@submodule/receipe-rest",
  "packageManager": "yarn@3.3.0",
  "types": "dist/index.d.ts",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "minify": false,
    "treeshake": false,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "dependencies": {
    "@submodule/cli": "workspace:^"
  },
  "scripts": {
    "build": "yarn run -T tsup",
    "receipe-rest:build": "yarn build"
  }
}
