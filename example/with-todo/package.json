{
  "name": "example-todo",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "dev": "tsx watch main",
    "build": "yarn run -T tsup",
    "start": "node dist/main"
  },
  "dependencies": {
    "@hono/node-server": "^0.2.4",
    "@submodule/core": "latest",
    "commander": "^10.0.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "hono": "^3.0.2",
    "level": "^8.0.0",
    "tsx": "^3.12.6"
  },
  "tsup": {
    "entry": [
      "./main.ts"
    ],
    "minify": true,
    "treeshake": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": false
  },
  "types": "submodule.router.ts"
}
